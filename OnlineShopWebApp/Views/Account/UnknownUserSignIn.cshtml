@model User

    <h2 class="text-center" style="font:bold; font-family:'Courier New'">Этот пользователь не найден, чтобы создать аккаунт, зарегистрируйтесь</h2>
    <hr class="m-4" />

    <div class="row justify-content-center">
        <form method="post">
            @await Html.PartialAsync("UserRegistration")
            <div class="text-center">
                <button type="submit" class="btn btn-outline-dark" asp-area="" asp-controller="Account" asp-action="Register">Зарегистрироваться</button>
            </div>
        </form>
    </div>

